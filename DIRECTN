#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;  cin>>t;
    while(t--){
        int n,flag=0;  cin>>n;
        string str; cin>>str;
        
        for(int i=0;i<n;i++)   if(str[i]==str[i+1])    flag=1;
        
        (flag==1)?cout<<"YES"<<endl:cout<<"NO"<<endl;
    }
   
    return 0;
}

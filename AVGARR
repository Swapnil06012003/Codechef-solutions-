#include <bits/stdc++.h>
using namespace std;

int main(){
    int t;  cin>>t;
    while(t--){
        int n,x,d=0;    cin>>n>>x;
        if(n%2==1) {
            d=-1*n/2;
            for(int i=0;i<n;i++){
                cout<<x+d<<" ";
                d++;
                
            }
            cout<<endl;
        }
        
        else{
            d=-1*n/2;
            for(int i=0;i<n;i++){
                cout<<x+d<<" ";
                if(d==-1)   d=1;
                else    d++;
            }
            cout<<endl;
        }
    }

    return 0;
}

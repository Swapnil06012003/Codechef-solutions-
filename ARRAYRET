#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;  cin>>t;
    while(t--){
        long long n;  cin>>n;
        long long sum=0;
        long long arr[n];
        for(int i=0;i<n;i++){    cin>>arr[i]; sum+=arr[i];   }
        long long  agent = sum/(n+1);
        long long ans[n];
        for(int i=0;i<n;i++){   ans[i]=arr[i]-agent;    cout<<ans[i]<<" ";    }
        cout<<endl;
        
    }
    return 0;
}
